<Window x:Class="NeuronInterface.Views.DataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NeuronInterface.Views"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="DataWindow" Height="450" Width="800">
    <Grid>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,10,125,23"/>
        <Button x:Name="Save" Content="Zapisz" HorizontalAlignment="Left" VerticalAlignment="Top" Height="42" Width="99" Margin="691,10,0,0"/>
        <Button x:Name="Return" Content="Wróć" HorizontalAlignment="Left" VerticalAlignment="Top" Height="42" Width="99" Margin="691,57,0,0"/>

    </Grid>
</Window>
